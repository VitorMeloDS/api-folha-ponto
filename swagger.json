{
	"swagger": "2.0",
    "info": {
      "title": "Folha de Ponto",
      "description": "Swagger API Folha de Pontoo",
      "version": "1.0"
    },
    "host": "localhost:2022",
    "basePath": "/",
    "schemes": [
      "http"
    ],
    "paths": {
      "/login": {
        "get": {
          "description": "Rota para Atenticação do Login.",
          "operationId": "login",
					"components":{
						"securitySchemes":{
							"BasicAuth": {
								"type": "http",
								"scheme": "basic"
							}
						}
					},
          "parameters": [
            {
              "name": "Usuário",
              "in": "path",
              "description": "Nome do usuário passado no Basic Auth <code>Vitor</code>.",
              "required": true,
              "default": "Vitor"
            },
            {
              "name": "Senha",
              "in": "path",
              "description": "Senha do usuário passada no Basic Auth <code>123456</code>.",
              "required": true,
              "default": "123456"
            }
          ],
          "responses": {}
        }
      }
    }   
}